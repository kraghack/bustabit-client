{
  "name": "bustabit-client",
  "version": "0.1.0",
  "homepage": "https://www.bustabit.com",
  "description": "Client for bustabit",
  "private": true,
  "engines": {
     "node": "8.x"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-preset-env": "^1.2.2",
    "bootstrap-sass": "^3.3.7",
    "font-awesome": "^4.6.3",
    "node-sass": "^4.5.0",
    "qrcode.react": "^0.7.1",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-addons-transition-group": "^15.4.2",
    "react-bootstrap": "^0.31.0",
    "react-confirm": "^0.1.14",
    "react-dom": "^15.4.2",
    "react-highcharts": "^12.0.0",
    "react-notification": "^6.6.0",
    "react-router": "^4.1.1",
    "react-scripts": "^1.0.7",
    "rollup": "^0.42.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "speakeasy": "^2.0.0",
    "timeago-react": "^1.0.7",
    "work-token": "^1.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "postinstall": "npm run sass; npm run build-iframe",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "sass": "node-sass --precision 8 src/sass/app.scss src/sass/app.css",
    "sass-dev": "node-sass --watch --precision 8 src/sass/app.scss src/sass/app.css",
    "eject": "react-scripts eject",
    "build-iframe": "cd iframe && node rollup.js"
  },
  "dependencies": {
    "eventemitter3": "^2.0.2",
    "rollup-plugin-commonjs": "^8.0.2"
